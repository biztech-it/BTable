     {
      "name": "resourceCode",
      "value": ".container {\n    width: 500px;\n    padding: 40px;\n    margin: auto;   \n}\n\n#header {\n    padding-bottom: 12px;\n}\n\n#logo a {\n    width: 185px;\n    height: 70px;\n\tdisplay: block;\n  \tbackground: url('static/custom/img/btable.png') 0px 50% no-repeat transparent;\n}\n\n.title {\n    font-family: Tahoma, Arial, sans-serif;\n    font-size: 24px;\n    line-height: 68px;\n    letter-spacing: 1px;\n}\n\n.label {\n    padding-top: 8px;\n    padding-bottom: 4px;\n    padding-left: 2px;\n    font-size: 13px;\n    font-weight: bold;\n    color: #404040;\n}\n\n.loadingIcon {\n    width: 24px;\n    height: 8px;\n    margin-left: 4px;\n    display: none;\n}\n\n#buttons {\n    padding-top: 16px;\n    text-align: center;\n}\n\nselect {\n    width: 500px;\n}\n\nbutton {\n    padding: 6px;\n    background-color: #F6F7F8;\n    border: 1px solid #AAAAAA;\n    border-radius: 4px 4px 4px 4px;\n    cursor: pointer;\n    font-family: Arial, sans-serif;\n    font-size: 13px;\n    font-weight: bold;\n    color: #444444;\n}\n\nbutton:hover {\n    border-color: #5897FB;\n    color: #5897FB;\n}\n\n#message {\n    padding-left: 4px;\n    font-size: 13px;\n    font-weight: normal;\n    color: red;    \n} ",
