     {
      "name": "resourceCode",
      "value": "var catalogs = [];\nvar cubes = [];\n\nfunction getCubeStructure(catalog, cube, jndi) {\n    \n    var cubeStructure = null;\n    \n    $.ajax({\n        type: \"GET\",\n        url: Dashboards.getWebAppPath() + \"/content/BTable/olapUtils\",\n        data: {\n            operation: \"GetCubeStructure\",\n            catalog: catalog,\n            cube: cube,\n            jndi: jndi\n        },\n        dataType: \"json\",\n        success: function(json) {\n            if(json && json.status == \"true\" && json.result) {\n                if(json.result.dimensions && json.result.measures)\n                    cubeStructure = json.result;\n            }\n        },\n        async: false\n    });\n    \n    return cubeStructure;\n    \n}\n\nfunction updateDimensionAndMeasureSelectors(catalogName, cubeName) {\n    var catalog = catalogs[catalogName]; \n    \n    if(cubes[catalogName + \"::\" + cubeName] === undefined) {\n        cubes[catalogName + \"::\" + cubeName] = getCubeStructure(catalog.schema, cubeName, catalog.jndi);\n    }\n    \n    var cube = cubes[catalogName + \"::\" + cubeName];\n        \n    if(cube !== null) {\n        \n        var measureSelectorObj = $(\"#measure\");\n        measureSelectorObj.empty();\n        \n        $.each(_.sortBy(cube.measures, function(e){return e.caption;}), function(i, v) {\n            measureSelectorObj.append(\"<option value='\" + v.qualifiedName + \"'>\" + v.caption + \"</option>\");            \n        });\n        \n        var dimensionSelectorObj = $(\"#dimension\");\n        dimensionSelectorObj.empty();\n        \n        $.each(_.sortBy(cube.dimensions, function(e){return e.caption;}), function(i, v) {\n            var html = \"\";\n            var dimensionName = v.caption;\n            \n            $.each(_.sortBy(v.hierarchies, function(e){return e.caption;}), function(i, v) {\n                var hierarchyName = v.caption.indexOf(dimensionName + \".\") === 0 ? v.caption.replace(dimensionName + \".\", \"\") : v.caption;\n                /*\n                if(hierarchyName.indexOf(\".\") > 0) {\n                    var parts = hierarchyName.split(\".\");\n                    if(parts[0] == parts[1])\n                        hierarchyName = parts[0];\n                }\n                */\n                html += \"<optgroup label='\" + dimensionName + (dimensionName == hierarchyName ? \"\" : \" :: \" + hierarchyName) + \"'>\";\n                \n                $.each(v.levels, function(i, v) {\n                    html += \"<option value='\" + v.qualifiedName + \"'>\" + v.caption + \"</option>\";\n                });\n                \n                html += \"</optgroup>\";\n            });\n                        \n            dimensionSelectorObj.append(html);            \n        });\n        \n        measureSelectorObj.trigger(\"liszt:updated\");\n        $(\"#measureLoadingIcon\").hide();\n        \n        dimensionSelectorObj.trigger(\"liszt:updated\");\n        $(\"#dimensionLoadingIcon\").hide();\n        \n    }\n\n}\n\nfunction updateCubeSelector(catalogName) {\n    \n    var catalog = catalogs[catalogName];\n    \n    $(\"#message\").html(\"\");\n    \n    var datasourceExists = false;\n\n    $.ajax({\n        type: \"GET\",\n        url: Dashboards.getWebAppPath() + \"/content/BTable/listCdaDatasources\",\n        data: {},\n        dataType: \"json\",\n        success: function(json) {\n            if(json && json.resultset) {\n                var schema = catalog.schema;\n                var jndi = catalog.jndi;\n                $.each(json.resultset, function(i, v) {\n                    if((\"solution:\" + v[0]) == schema && v[1] == jndi)\n                        datasourceExists = true;\n                });\n            }\n        },\n        async: false\n    });\n    \n    if(!datasourceExists) {\n        $(\"#message\").html(\"A BTable datasource for the selected catalog does not exists!\");\n    }\n    \n    var cubeSelectorObj = $(\"#cube\");\n    cubeSelectorObj.empty();\n    \n    var _cubes = _.sortBy(catalog.cubes, function(e){return e.caption;});\n    \n    $.each(_cubes, function(i, v) {\n        cubeSelectorObj.append(\"<option value='\" + v.id + \"'>\" + v.caption + \"</option>\");            \n    });\n    \n    cubeSelectorObj.trigger(\"liszt:updated\");\n    \n    $(\"#cubeLoadingIcon\").hide();\n    \n    if(_cubes.length > 0) {\n        updateDimensionAndMeasureSelectors(catalogName, _cubes[0].id);   \n    }\n    \n}\n\nfunction initSelectors(_catalogs) {\n        \n    var catalogSelectorObj = $(\"#catalog\");\n    \n    catalogSelectorObj.empty();\n    \n    _catalogs = _.sortBy(_catalogs, function(e){return e.name;});\n    \n    $.each(_catalogs, function(i, v) {\n        var name = v.name;\n        var jndi = v.jndi;\n        var schema = v.schema;\n        var cubes = v.cubes;\n        catalogs[name] = {\n            jndi: jndi,\n            schema: schema,\n            cubes: cubes\n        };\n        \n        catalogSelectorObj.append(\"<option value='\" + name + \"'>\" + name + \"</option>\");        \n    });\n    \n    $(\"#catalogLoadingIcon\").hide();\n    \n    updateCubeSelector(_catalogs[0].name);\n\n\n    catalogSelectorObj.change(function() {\n        $(\"#cubeLoadingIcon\").show();\n        $(\"#measureLoadingIcon\").show();\n        $(\"#dimensionLoadingIcon\").show();\n        \n        var selectedCatalogName = $(this).val();\n        updateCubeSelector(selectedCatalogName);\n    });\n\n    $(\"#cube\").change(function() {\n        $(\"#measureLoadingIcon\").show();\n        $(\"#dimensionLoadingIcon\").show();\n\n        var selectedCatalogName = catalogSelectorObj.val();\n        var selectedCubeName = $(this).val();\n        updateDimensionAndMeasureSelectors(selectedCatalogName, selectedCubeName);\n    });\n\n    $(\".chosen-select\").chosen();\n\n}\n\n$(document).ready(function() {\n    \n    $(\".loadingIcon\").show();\n    \n    $.ajax({\n        type: \"GET\",\n        url: Dashboards.getWebAppPath() + \"/content/BTable/olapUtils?operation=GetOlapCubes\",\n        data: {},\n        dataType: \"json\",\n        success: function(json) {\n            if(json && json.status == \"true\" && json.result) {\n                if(json.result.catalogs && json.result.catalogs.length > 0)\n                    initSelectors(json.result.catalogs);\n            } else {\n                $(\".loadingIcon\").hide();\n            }\n        },\n        async: true\n    });\n    \n    $(\"#open\").click(function() {\n        var selectedCatalogName = $(\"#catalog\").val();\n        var catalog = catalogs[selectedCatalogName];\n        \n\t\tvar bTableProp = {};\n\t\tbTableProp.catalog = catalog.schema.replace(\"solution:\", \"\");\n\t\tbTableProp.jndi = catalog.jndi;\n\t\tbTableProp.cube = $(\"#cube\").val();\n\t\tbTableProp.dimensions = [[$(\"#dimension\").val(), \"\"]];\n\t\tbTableProp.measures = [[$(\"#measure\").val(), \"\"]];\n\n        var url = Dashboards.getWebAppPath() + \"/content/BTable/render?btdef=\" + encodeURIComponent(JSON.stringify(bTableProp));\n        if(Dashboards.getQueryParameter(\"debug\") == \"true\") url += \"&debug=true\";\n        window.open(url, \"_self\"/*\"_blank\"*/);        \n    });\n    \n});\n ",
      "type": "Resource"
     }
    ]
